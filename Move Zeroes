class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        vector<int> v;
        int cz=0;
        for(int i=0;i<nums.size();i++)
        {   if(nums[i]!=0)
                v.push_back(nums[i]);
            else
                cz++;
        }
        for(int i=0;i<cz;i++)
            v.push_back(0);
        nums=v;

    }
};
