class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int> st;
        for(int i=0;i<nums.size();i++)
        {
            st.insert(nums[i]);
        }
        vector<int> ans;
        auto it=st.begin();
        for(int i=0;i<st.size();i++)
        {
            ans.push_back(*it);
            it++;
        }
        nums=ans;
        return st.size();
    }
};
